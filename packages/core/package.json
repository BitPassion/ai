{
  "name": "ai-connector",
  "version": "0.0.6",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"src/**/*.ts*\"",
    "test": "jest --env @edge-runtime/jest-environment .test.ts && jest --env node .test.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "eventsource-parser": "1.0.0",
    "nanoid": "^3.3.6",
    "swr": "2.1.5"
  },
  "devDependencies": {
    "@edge-runtime/jest-environment": "1.1.0-beta.31",
    "@types/jest": "29.2.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vercel/ai-tsconfig": "workspace:*",
    "eslint": "^7.32.0",
    "eslint-config-vercel-ai": "workspace:*",
    "jest": "29.2.1",
    "react": "^17.0.2",
    "ts-jest": "29.0.3",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "engines": {
    "node": ">=14.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
